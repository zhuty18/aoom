{
  "globalQuery": "PATH DOES NOT INCLUDE _templates\nSHORT\nSORT BY status.type\nHIDE TASK COUNT\nHIDE BACKLINK\nHIDE POSTPONE BUTTON\nSHOW TREE",
  "globalFilter": "📚",
  "removeGlobalFilter": true,
  "taskFormat": "tasksPluginEmoji",
  "setCreatedDate": false,
  "setDoneDate": true,
  "setCancelledDate": true,
  "autoSuggestInEditor": false,
  "autoSuggestMinMatch": 0,
  "autoSuggestMaxItems": 20,
  "provideAccessKeys": true,
  "useFilenameAsScheduledDate": false,
  "filenameAsScheduledDateFormat": "",
  "filenameAsDateFolders": [],
  "recurrenceOnNextLine": false,
  "statusSettings": {
    "coreStatuses": [
      {
        "symbol": " ",
        "name": "待办",
        "nextStatusSymbol": "*",
        "availableAsCommand": true,
        "type": "TODO"
      },
      {
        "symbol": "x",
        "name": "完成",
        "nextStatusSymbol": " ",
        "availableAsCommand": true,
        "type": "DONE"
      }
    ],
    "customStatuses": [
      {
        "symbol": "*",
        "name": "当前",
        "nextStatusSymbol": "x",
        "availableAsCommand": false,
        "type": "IN_PROGRESS"
      },
      {
        "symbol": "!",
        "name": "重要",
        "nextStatusSymbol": "x",
        "availableAsCommand": false,
        "type": "IN_PROGRESS"
      },
      {
        "symbol": "I",
        "name": "创意",
        "nextStatusSymbol": " ",
        "availableAsCommand": false,
        "type": "NON_TASK"
      },
      {
        "symbol": "/",
        "name": "暂停",
        "nextStatusSymbol": "*",
        "availableAsCommand": false,
        "type": "IN_PROGRESS"
      },
      {
        "symbol": "-",
        "name": "取消",
        "nextStatusSymbol": " ",
        "availableAsCommand": false,
        "type": "CANCELLED"
      }
    ]
  },
  "features": {
    "INTERNAL_TESTING_ENABLED_BY_DEFAULT": true
  },
  "generalSettings": {},
  "headingOpened": {
    "Core Statuses": true,
    "Custom Statuses": true,
    "核心状态": true,
    "自定义状态": true
  },
  "debugSettings": {
    "ignoreSortInstructions": false,
    "showTaskHiddenData": false,
    "recordTimings": false
  },
  "loggingOptions": {
    "minLevels": {
      "": "info",
      "tasks": "info",
      "tasks.Cache": "info",
      "tasks.Events": "info",
      "tasks.File": "info",
      "tasks.Query": "info",
      "tasks.Task": "info"
    }
  }
}